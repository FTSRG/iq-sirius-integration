package hu.bme.mit.inf.dipterv1.incquery.patterns

import "http://hu.bme.mit.inf.models/socialnetwork_base"


pattern child(child : Person) {
	SocialNetwork.members(_, child);
	
	Person.age(child, age);
	
	eval (age < 18) == true;
}

pattern adult(adult : Person) {
	SocialNetwork.members(_, adult);
	
	Person.age(adult, age);
	
	eval (age >= 18) == true;	
}